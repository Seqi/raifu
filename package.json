		"@types/node": "^14.14.32",
		"@types/qs": "^6.9.6",
		"@types/react": "^17.0.1",
		"@types/react-big-calendar": "^0.30.1",
		"@types/react-dom": "^17.0.1",
		"@types/react-router-dom": "^5.1.2",
		"core-js": "^2.6.9",
		"firebase": "^8.2.2",
		"marked": "^0.8.2",
		"moment": "^2.24.0",
		"moment-range": "^4.0.2",
		"qs": "^6.5.2",
		"react": "^17.0.1",
		"react-big-calendar": "^0.33.2",
		"react-cookie": "^4.0.1",
		"react-dom": "^17.0.1",
		"react-hook-form": "^6.15.4",
		"react-router-dom": "^5.1.2",
		"react-scripts": "^4.0.3",
		"title-case": "^3.0.3",
		"typescript": "^4.2.3",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"concurrently": "^6.4.0",
		"prettier": "^2.2.1"
	},
	"scripts": {
		"start": "concurrently -n web,firebase,nest -c \"blue.bold,yellow.bold,red.bold\" --kill-others \"npm:web\" \"npm:firebase\" \"npm:functions:build\"",
		"web": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"firebase": "firebase emulators:start --inspect-functions --export-on-exit --import .emulator-data",
		"deploy": "firebase deploy",
		"eject": "react-scripts eject",
		"lint": "eslint src/**/*.js",
		"lint-fix": "eslint src/**/*.js --fix",
		"init": "npm i && npm --prefix functions i",
		"functions:start": "concurrently -n firebase,nest -c \"yellow.bold,red.bold\" --kill-others \"npm:firebase\" \"npm:functions:build\"",
		"functions:build": "npm --prefix functions run build:watch"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
